{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["esnext", "DOM"],
    "baseUrl": "./",
    "module": "esnext",
    "moduleResolution": "Node",
    "paths": {
      "@/*": ["./src/*"]
    },
    "resolveJsonModule": true,
    "typeRoots": ["./type"],
    "types": [],
    "strict": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true
  }
}
